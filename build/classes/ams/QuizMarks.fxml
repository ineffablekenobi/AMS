<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="ams.QuizMarksController">
   <children>
      <TableView fx:id="quizMarksTableView" layoutX="200.0" layoutY="26.0" prefHeight="360.0" prefWidth="386.0" />
      <TextField fx:id="txtFieldCourseName" layoutX="36.0" layoutY="59.0" promptText="Course name" />
      <TextField fx:id="txtFieldQuizNo" layoutX="36.0" layoutY="92.0" promptText="Quiz No" />
      <Button fx:id="btnFindMarks" layoutX="30.0" layoutY="168.0" mnemonicParsing="false" onAction="#onClickFindMarks" prefHeight="25.0" prefWidth="75.0" text="Find Marks" />
      <TextField fx:id="txtFieldSID" layoutX="36.0" layoutY="125.0" promptText="Student ID" />
      <Button fx:id="btnReset" defaultButton="true" layoutX="119.0" layoutY="168.0" mnemonicParsing="false" onAction="#onClickReset" prefHeight="25.0" prefWidth="75.0" text="Reset" />
      <ComboBox fx:id="dropDownFindBy" layoutX="35.0" layoutY="25.0" onAction="#onClickDropDown" prefWidth="150.0" promptText="Find By" />
   </children>
</AnchorPane>
